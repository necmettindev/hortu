{
  "name": "hortu",
  "version": "0.0.1",
  "description": "Hortu is a micro-performance focused alternative to Lodash, fully compatible with TypeScript.",
  "keywords": [
    "lodash",
    "lodash alternative",
    "lodash replacement",
    "lodash alternative typescript",
    "lodash replacement typescript",
    "lodash alternative performance",
    "lodash replacement performance",
    "lodash alternative performance typescript",
    "lodash replacement performance typescript",
    "lodash alternative typescript performance",
    "modular",
    "performance",
    "typescript",
    "type-safe",
    "runtime",
    "runtime type checking",
    "parsing",
    "validation"
  ],
  "author": "Necmettin <hi@necmettin.dev>",
  "homepage": "https://hortu.necmettin.dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/necmettindev/hortu"
  },
  "scripts": {
    "playground": "node ./playground.js",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint \"src/**/*.ts*\" && tsc --noEmit",
    "format": "prettier --write ./src",
    "format.check": "prettier --check ./src",
    "build": "tsup",
    "publish": "npm publish"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.2",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitest/coverage-v8": "^0.34.3",
    "eslint": "^8.47.0",
    "jsdom": "^22.1.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.3"
  }
}
